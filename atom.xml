  <entry>
    <title>Appreciation of Novels(1)</title>
    <link href="https://ning-hui.github.io/Appreciation%20of%20Novels/"/>
    <id>https://ning-hui.github.io/Appreciation of Novels/</id>
    <published>2019-06-20T05:48:53.000Z</published>
